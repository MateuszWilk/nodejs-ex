{
  "name": "nodejs-ex",
  "version": "0.0.1",
  "description": "Node.js sample app for OpenShift 3",
  "main": "server.js",
  "dependencies": {
    "body-parser": "^1.13.3",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "mocha": "^2.4.5",
    "mongodb": "^2.1.16",
    "mongoose": "^5.0.16",
    "morgan": "^1.7.0",
    "object-assign": "4.1.0",
    "resource-router-middleware": "^0.6.0",
    "restful-mongoose": "^1.0.1",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
